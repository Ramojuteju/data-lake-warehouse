			"type": "string",
			"defaultValue": "https://synastorage23.dfs.core.windows.net/"
		},
		"synapse23-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synastorage23.dfs.core.windows.net"
		}
