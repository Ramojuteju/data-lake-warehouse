			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newretailsynapse-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:newretailsynapse.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"retailsynapse23-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
