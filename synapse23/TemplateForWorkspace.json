			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE TABLE dbo.Sales (\n    [OrderID] nvarchar(400),\n    [CustomerID] nvarchar(400),\n    [EmployeeID] nvarchar(400),\n    [OrderDate] nvarchar(400),\n    [ShipperID] nvarchar(400)\n);\n\n",
